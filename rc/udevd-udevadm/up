#!/bin/execlineb -P

fdmove -c 2 1
if { s6-echo -- udevd-udevrules started }
if {
        foreground { /sbin/udevadm trigger --action=add --type=subsystems }
        foreground { /sbin/udevadm trigger --action=add --type=devices }
        /sbin/udevadm settle
}
s6-echo -- udevd-udevrules successfully started
