#!/bin/execlineb -P

# Remove any fielsystem watermarks and other data

fdmove -c 2 1
if { s6-echo -- [ .FSwtr.. ] Clearing ... }
foreground {
  install -m0644 -o root -g utmp /dev/null /run/utmp
}

foreground {
  rm -fv /etc/nologin /forcecheck /forcequotacheck /fastboot 
}
s6-echo -- [ .FSwtr.. ] Done. 
